{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "minimum-stability": "dev",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lara-zeus/translatable"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-intl": "*",
        "bezhansalleh/filament-shield": "*",
        "codeat3/blade-google-material-design-icons": "^1.19",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-tags-plugin": "^3.2",
        "filament/spatie-laravel-translatable-plugin": "3.x-dev",
        "florianv/swap": "^4.3",
        "itsgoingd/clockwork": "^5.3",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "malzariey/filament-daterangepicker-filter": "^3.1",
        "mohamedsabil83/filament-forms-tinyeditor": "^2.3",
        "nyholm/psr7": "^1.8",
        "pelmered/filament-money-field": "^1.4",
        "php-http/curl-client": "^2.3",
        "php-http/guzzle7-adapter": "^1.0",
        "ralphjsmit/laravel-seo": "^1.6",
        "spatie/laravel-google-fonts": "^1.4",
        "statikbe/laravel-filament-chained-translation-manager": "^3.2",
        "symfony/cache": "^7.1",
        "symfony/dom-crawler": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.3",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "files": [
            "app/Http/Helpers/money.php",
            "app/Http/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "prefer-stable": true
}
